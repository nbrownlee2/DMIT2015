package ca.nait.dmit.domain;

/**
 * 
 * @author: Nicholas Brownlee
 *
 */

public class BMI {

	private int weight;
	private int height;
	
	public BMI(){
		this.weight = 0;
		this.height = 0;
	}
	
	public BMI(int weight, int height) {
		super();
		this.weight = weight;
		this.height = height;
	}

	public int getWeight() {
		return weight;
	}

	public void setWeight(int weight) {
		this.weight = weight;
	}

	public int getHeight() {
		return height;
	}

	public void setHeight(int height) {
		this.height = height;
	}		
}

public bmi(int height, int weight) {
	return BMI;
}
